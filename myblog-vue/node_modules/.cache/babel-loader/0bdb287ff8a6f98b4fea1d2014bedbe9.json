label=\"内容\" name=\"content\">\r\n                    <v-md-editor v-model=\"formState.content\" height=\"400px\"></v-md-editor>\r\n                </a-form-item>\r\n                <a-form-item :wrapper-col=\"{ span: 18, offset: 10 }\">\r\n                    <a-button type=\"primary\" @click=\"onSubmit\">创建</a-button>\r\n                    <a-button style=\"margin-left: 10px\" @click=\"resetForm\">重置</a-button>\r\n                </a-form-item>\r\n            </a-form>\r\n        </div>\r\n    </a-layout-content>\r\n    </a-layout>\r\n</template>\r\n\r\n<script>\r\n    import Header from '../components/Header'\r\n    import {defineComponent, ref, getCurrentInstance, reactive, onMounted, toRaw} from 'vue'\r\n    import {Modal, message} from \"ant-design-vue\";\r\n    export default defineComponent({\r\n        name: \"EditBlogs\",\r\n        components:{\r\n            Header,\r\n        },\r\n        setup(){\r\n            const {proxy} = getCurrentInstance()\r\n            const formRef = ref();\r\n            const blog = reactive({\r\n                blog:{\r\n                }\r\n            })\r\n            const formState = reactive({\r\n                title: '',\r\n                description: '',\r\n                content: '',\r\n            });\r\n            let text = ref('')\r\n            const rules = {\r\n                title: [\r\n                    {\r\n                        min: 1,\r\n                        message: '标题最少1字符',\r\n                        trigger: 'blur',\r\n                    },\r\n                    {\r\n                        validator: (rule, value, callback) => {\r\n                            if(value.trim() === ''){\r\n                                return Promise.reject('标题不能为空')\r\n                            }else{\r\n                                return Promise.resolve()\r\n                            }\r\n                        },\r\n                        trigger: 'blur',\r\n                    }\r\n                ],\r\n                description: [\r\n                    {\r\n                        required: true,\r\n                        message: '描述不能为空',\r\n                        trigger: 'blur',\r\n                    },\r\n                ],\r\n                content: [\r\n                    {\r\n                        required: true,\r\n                        message: '内容不能为空',\r\n                        trigger: 'blur',\r\n                    },\r\n                ],\r\n            };\r\n            const onSubmit = () => {\r\n                formRef.value\r\n                    .validate()\r\n                    .then(() => {\r\n                        let formData = reactive({data:{}})\r\n                        if(blog.blog.id !== undefined){\r\n                            formData.data = blog.blog\r\n                        }else{\r\n                            formData.data = toRaw(formState)\r\n                        }\r\n                        console.log(formData.data)\r\n                        console.log(blog.blog.id)\r\n                        proxy.$axios.post('/edit', formData.data, {\r\n                            headers: {\r\n                                \"Authorization\": localStorage.getItem('token')\r\n                            }\r\n                        }).then(res => {\r\n                            if(res.data.status === 200){\r\n                                message.success('博客创建成功', 2)\r\n\r\n                            }else{\r\n                                message.error('博客创建失败', 2)\r\n                            }\r\n                        }).catch(error => {\r\n                            message.error(error.message)\r\n                        })\r\n                    })\r\n                    .catch(error => {\r\n                        console.log('error', error);\r\n                    });\r\n            };\r\n\r\n            const resetForm = () => {\r\n                formRef.value.resetFields();\r\n            };\r\n            const getBlog = () =>{\r\n                const blogId = proxy.$route.params.blogId\r\n                if(blogId !== null && blogId !== undefined){\r\n                    console.log(blogId)\r\n                    proxy.$axios.get('/detail/' + blogId).then(res => {\r\n\r\n                        blog.blog = res.data.data\r\n                        formState.title = blog.blog.title\r\n                        formState.content = blog.blog.content\r\n                        formState.description = blog.blog.description\r\n                        console.log(blog.blog)\r\n                    })\r\n                }\r\n            }\r\n\r\n            onMounted(() => {\r\n                getBlog()\r\n            })\r\n\r\n            return {\r\n                formRef,\r\n                labelCol: {\r\n                    span: 3,\r\n                },\r\n                wrapperCol: {\r\n                    span: 18,\r\n                },\r\n                other: '',\r\n                formState,\r\n                rules,\r\n                onSubmit,\r\n                resetForm,\r\n                text,\r\n            };\r\n        }\r\n    })\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":""}]}